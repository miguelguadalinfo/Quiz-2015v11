<form method="get" action="/quizes">	<input type="text" name="search" value="Búsqueda" />	<input type="submit" value="Enviar"></form><% if (quizes.length === 0) { %>	<p>No hay preguntas</p><% } %><% if (quizes.length > 0) { %>	<table>		<% var i; for (i=0; i < quizes.length; i++) { %>			<tr>				<td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>			</tr> 		<% } %>	</table><% } %><p><a href="/quizes/new"><button type="button">Crear pregunta</button></a> </p>